<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>Hooks for dead-simple data fetching with Feliz</Description>
        <PackageIconUrl></PackageIconUrl>
        <PackageTags>fsharp;fable;react;html;feliz</PackageTags>
        <Authors>Zaid Ajaj</Authors>
        <Version>2.0.0</Version>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageReleaseNotes>Update Feliz dependency for Feliz v2.0</PackageReleaseNotes>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="UseDeferred.fs" />
    </ItemGroup>
    <ItemGroup>
        <Content Include="*.fsproj; *.fs; *.js;" Exclude="**\*.fs.js" PackagePath="fable\" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Feliz" Version="2.9.0" />
    </ItemGroup>
</Project>