        "css-loader": "^5.2.4",
        "fable-compiler": "^2.11.0",
        "fable-loader": "^2.1.9",
        "gh-pages": "^2.2.0",
        "mocha": "^6.2.3",
        "remotedev": "^0.2.7",
        "sass": "^1.32.4",
